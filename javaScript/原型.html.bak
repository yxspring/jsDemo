<script>
var arr1=new Array(1,2,3,4);
var arr2=new Array(10,12,13,15);
Array.prototype.sum=function(){
//arr1.sum=function(){//行间样式
	var result=0;
	for(var i=0;i<this.length;i++){	
		result+=this[i];
	}
	return result;
};
